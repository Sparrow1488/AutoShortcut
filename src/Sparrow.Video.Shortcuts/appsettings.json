{
  "Assembly": {
    "Name": "AutoShortcut",
    "Description": "Small video editor will help you automate boring and long work.",
    "Author": "Sparrow1488",
    "Version": "0.3.0"
  },
  "Environment": {
    "Current": "Development",
    "Mode": "Release",
    "Paths": {
      "Files": "saved\\files",
      "FilesMeta": "saved\\meta",
      "FormattedFiles": "processed\\formatted",
      "SilentFiles": "processed\\silent",
      "EncodedFiles": "processed\\encoded",
      "ScaledFiles": "processed\\scaled",
      "ResultFiles": "processed\\output",
      "Scripts": "processed\\scripts"
    }
  },
  "Output": {
    "Text": {
      "MaxFileNameChars": 25
    }
  },
  "Processes": {
    "Analyse": {
      "ffprobe": "C:\\Users\\USER\\Downloads\\ffmpeg-master-latest-win64-gpl\\ffmpeg-master-latest-win64-gpl\\bin\\ffprobe.exe"
    },
    "Video": {
      "ffmpeg": "C:\\Users\\USER\\Downloads\\ffmpeg-master-latest-win64-gpl\\ffmpeg-master-latest-win64-gpl\\bin\\ffmpeg.exe"
    }
  },
  "Store": {
    "FileTypes": {
      "Video": {
        "Extensions": [
          ".mp4",
          ".mov",
          ".wmv",
          ".avi",
          ".ts"
        ]
      },
      "Image": {
        "Extensions": [
          ".png",
          ".jpeg",
          ".jpg",
          ".gif"
        ]
      },
      "Audio": {
        "Extensions": [
          ".mp3",
          ".m4a",
          ".aac"
        ]
      }
    }
  }
}